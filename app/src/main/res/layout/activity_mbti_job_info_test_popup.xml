<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mbti_job_info_test_popup">


    <LinearLayout
        android:layout_width="340dp"
        android:layout_height="540dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="#EDEDE4"
            android:src="@drawable/catbti"/>

        <TextView
            android:id="@+id/testpopTitleText"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/popuptextview"
            android:gravity="center"
            android:text="TextView"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/EIText"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/popuptextview"
            android:gravity="center"
            android:text="당신의 E(외향형)와 I(내향형)의 결과는"
            android:textColor="#000000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/testEText"
                android:layout_width="170dp"
                android:background="@drawable/popuptextview"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="TextView"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/testIText"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:background="@drawable/popuptextview"
                android:backgroundTint="#EDEDE4"
                android:text="TextView"
                android:gravity="center"
                android:textColor="#000000" />

        </LinearLayout>

        <TextView
            android:id="@+id/EIText2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="당신의 S(감각형)와 N(직관형)의 결과는"
            android:background="@drawable/popuptextview"
            android:textColor="#000000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/testSText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/popuptextview"
                android:gravity="center"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/testNText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/popuptextview"
                android:gravity="center"
                android:layout_weight="1"
                android:text="TextView"
                android:textColor="#000000" />
        </LinearLayout>

        <TextView
            android:id="@+id/EIText3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="당신의 T(사고형)와 F(감정형)의 결과는"
            android:background="@drawable/popuptextview"
            android:textColor="#000000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/testTText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="1"
                android:text="TextView"
                android:background="@drawable/popuptextview"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/testFText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="1"
                android:text="TextView"
                android:background="@drawable/popuptextview"
                android:textColor="#000000" />
        </LinearLayout>

        <TextView
            android:id="@+id/EIText4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/popuptextview"
            android:gravity="center"

            android:text="당신의 J(판단형)와 P(인식형)의 결과는"
            android:textColor="#000000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/testJText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="TextView"
                android:gravity="center"
                android:background="@drawable/popuptextview"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/testPText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="TextView"
                android:gravity="center"
                android:background="@drawable/popuptextview"
                android:textColor="#000000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/gotoJobInfoBtn"
                style="@style/FirebaseUI.Button"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:background="#CC7161"
                android:text="검사완료"
                android:textColor="#FFFFFF"
                tools:ignore="TextContrastCheck" />

            <Button
                android:id="@+id/testPopupCloseBtn"
                style="@style/FirebaseUI.Button"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:background="#E5AC8E"
                android:text="닫기"
                android:textColor="#FFFFFF"
                tools:ignore="TextContrastCheck" />

        </LinearLayout>

    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>