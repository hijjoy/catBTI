<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEDE4"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/highlighter"
        android:layout_width="259dp"
        android:layout_height="96dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.305"
        app:srcCompat="@drawable/highlighter" />

    <TextView
        android:id="@+id/mbtiInfoView"
        android:layout_width="258dp"
        android:layout_height="85dp"
        android:fontFamily="@font/darae_1minute"
        android:gravity="center_horizontal"
        android:text="MBTI"
        android:textColor="#59041d"
        android:textSize="65sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <ImageView
        android:id="@+id/cat"
        android:layout_width="146dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171"
        app:srcCompat="@drawable/catbti" />

    <Button
        android:id="@+id/logout_btn_google"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/drfont_basic"
        android:text="로그아웃"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/wordBubble"
        android:layout_width="332dp"
        android:layout_height="255dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.529"
        app:srcCompat="@drawable/word" />

    <Button
        android:id="@+id/testButton"
        android:layout_width="202dp"
        android:layout_height="56dp"
        android:background="#00ff0000"
        android:fontFamily="@font/jua"
        android:text="검사하기"
        android:textColor="#000000"
        android:textSize="43sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mbtiInfoView"
        app:layout_constraintVertical_bias="0.184" />

    <Button
        android:id="@+id/boardButton"
        android:layout_width="82dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="게시판"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7"
        tools:ignore="TouchTargetSizeCheck" />


    <Button
        android:id="@+id/boardButton2"
        android:layout_width="82dp"
        android:layout_height="43dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="게시판2"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.962" />

    <ImageView
        android:id="@+id/redstar"
        android:layout_width="44dp"
        android:layout_height="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.757"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.353"
        app:srcCompat="@drawable/redstar" />

    <Button
        android:id="@+id/mbtiButton"
        android:layout_width="110dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="MBTI란?"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.788" />

    <Button
        android:id="@+id/mbtiJobButton"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="MBTI - 직업"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.877"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/mbtiFPostBtn"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#EDEDE4"
        android:fontFamily="@font/jua"
        android:gravity="center"
        android:text="MBTI 밸런스 게임"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/yellowstar"
        android:layout_width="42dp"
        android:layout_height="41dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.266"
        app:srcCompat="@drawable/yellowstar" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="36dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.331"
        app:srcCompat="@drawable/orangestar" />


</androidx.constraintlayout.widget.ConstraintLayout>